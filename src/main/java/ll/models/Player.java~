package ll.models;

import javax.persistence.Entity;
import javax.persistence.Id;


@Entity
public class Player  {
    @Id
    private String id;
    private String name;
    private String url;


    public String getId() {
	return id;
    }
    
    public void setId(String id){
	this.id = id;
    }

    public String getName(){
	return name;
    }

    public  String getUrl(){
	return url;
    }
    public Player (){
    }
    public Player (String id, String name , String url){
	this.id = id;
	this.name = name;
	this.url = url;
    }
    
    
}
